{
  "name": "vaulted",
  "version": "1.1.0",
  "description": "A nodejs wrapper library for using HashiCorp's Vault",
  "main": "index.js",
  "scripts": {
    "start": "npm rebuild && node index.js",
    "test": "mocha -R spec tests/**/*.js",
    "coverage": "istanbul cover _mocha tests/**/*.js",
    "test-watch": "mocha --growl -R spec -w tests/**/*.js"
  },
  "keywords": [
    "vault",
    "token",
    "auth",
    "secrets",
    "hashicorp"
  ],
